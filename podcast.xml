---
layout: none
xmlpage: yes
---
<?xml version="1.0" encoding="UTF-8"?>
<rss xmlns:itunes="http://www.itunes.com/dtds/podcast-1.0.dtd" xmlns:atom="http://www.w3.org/2005/Atom" version="2.0">
	<channel>
		<title>Thrideo</title>
		<atom:link href="{{ "/podcast.xml" | prepend: site.github.url }}" rel="self" type="application/rss+xml" />
		{% include head.xml %}
		{% for post in site.categories.video limit:40 %}
			<item>
				<title>{% if post.photo-file %}Photo: {% endif %}{{ post.title | xml_escape }}</title>
				<enclosure url="{{ site.sd-video-server-domain }}thrideo/{{ post.catid }}/{{ post.file }}-SD.m4v" type="video/x-m4v" length="{% if post.length %}{{ post.length.sd }}{% else %}0{% endif %}"  />
				{% include description.xml %}
			</item>
		{% endfor %}
		<item>
			<title>More...</title>
			<description>
			<![CDATA[
				<p><a href="http://itunes.apple.com/podcast/id918737148?at=10lJIS">Upgrade to 1080p version</a></p>
				<h2>Archive</h2>
				<ul>
				{% for post in site.posts offset:1 %}
				<li>
				<a href="{{ post.url | prepend: site.github.url }}"><b>{{ post.date | date: "%b %d %Y" }}</b><span> {{ post.title }}</span></a>
				</li>
				{% endfor %}
				</ul>
			]]>
			</description>
			<guid isPermaLink="false">more</guid>
			<itunes:block>yes</itunes:block>
			<enclosure url="https://thrideo.tlo.xyz/" type="text/xml" length="0" />
		</item>
	</channel>
</rss>
